<project>
  <extend>${basedir}/../app-base/project.xml</extend>
  <artifactId>mmbase-resources</artifactId>
  <name>Resources</name>
  <shortDescription>MMBase Resources</shortDescription>
  <description>
    This application provides builders and utility code for some very often reused object types. 
    You are very encouraged to use this application always in your MMBase applications.
  </description>
  <properties>
    <type>library</type>
    <status>stable</status>
  </properties>
  <dependencies>
    <dependency>
      <groupId>jai</groupId>
      <artifactId>jai_codec</artifactId>
      <version>1.1.2_01</version>
      <type>jar</type>
      <url>http://java.sun.com/products/java-media/jai/index.jsp</url>
      <properties>
        <war.bundle>${war.bundle}</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>jai</groupId>
      <artifactId>jai_core</artifactId>
      <version>1.1.2_01</version>
      <type>jar</type>
      <url>http://java.sun.com/products/java-media/jai/index.jsp</url>
      <properties>
        <war.bundle>${war.bundle}</war.bundle>
      </properties>
    </dependency>
  </dependencies>
</project>

