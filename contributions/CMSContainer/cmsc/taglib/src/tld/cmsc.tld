<!-- ############## -->
<!-- HTML Form Tags -->	
<!-- ############## -->
	<tag>
		<description>Generate html select tag</description>
		<name>select</name>
		<tag-class>com.finalist.cmsc.taglib.form.SelectTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable to set selected.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Generate html option tag</description>
		<name>option</name>
		<tag-class>com.finalist.cmsc.taglib.form.OptionTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>key in Resourcebundle.</description>
			<name>message</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>name attribute of html option.</description>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>value attribute of html option.</description>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Generate html input text tag</description>
		<name>text</name>
		<tag-class>com.finalist.cmsc.taglib.form.TextTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable to set value.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>default value.</description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Writes the version of the application which is implementing the container</description>
		<name>version</name>
		<tag-class>com.finalist.cmsc.taglib.VersionTag</tag-class>
		<body-content>scriptless</body-content>
	</tag>
	<tag>
		<description>Dumps the data of the framework builders to a certain path on the server.</description>
		<name>dumpdefaults</name>
		<tag-class>com.finalist.cmsc.taglib.DumpDefaultsTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>the path to dump to.</description>
			<name>path</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Writes a property to the output</description>
		<name>property</name>
		<tag-class>com.finalist.cmsc.taglib.PropertyTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>the key of the property.</description>
			<name>key</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>JSP variable name to put the result.</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Removes all html tags from a tekst</description>
		<name>removehtml</name>
		<tag-class>com.finalist.cmsc.taglib.RemoveHtmlTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>The html to put into the tag.</description>
			<name>html</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>JSP variable name to put the result.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>The maximum length allowed, will be cut back to the nearest '.', if not found to the nearest space.</description>
			<name>maxlength</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	
