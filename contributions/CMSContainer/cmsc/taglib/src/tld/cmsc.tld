<?xml version="1.0" encoding="ISO-8859-1" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd" version="2.0">

	<description>Taglib to support the Finalist CMS Container</description>
	<display-name>CMS Container tags</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>cmsc</short-name>
	<uri>http://finalist.com/csmc</uri>
	<tag>
		<description>Container for a Screen(Fragment) and it's Layout template</description>
		<name>screen</name>
		<tag-class>com.finalist.cmsc.taglib.ScreenTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put a screen in.</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Tag to show a Screen's title</description>
		<name>title</name>
		<tag-class>com.finalist.cmsc.taglib.TitleTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>JSP variable name to put the title in.</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Include the title of the site</description>
			<name>site</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Tag to insert a portlet</description>
		<name>insert-portlet</name>
		<tag-class>com.finalist.cmsc.taglib.InsertPortletTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>JSP variable to use some day.</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Display portlet with this layoutid.</description>
			<name>layoutid</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Container for protected content</description>
		<name>protected</name>
		<tag-class>com.finalist.cmsc.taglib.ProtectedTag</tag-class>
		<body-content>scriptless</body-content>
	</tag>
	<tag>
		<description>Injects content from portlets in the header of the page</description>
		<name>headercontent</name>
		<tag-class>com.finalist.cmsc.taglib.HeaderContentTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>use dublin meta ("true" or "false") (defaults to false)</description>
			<name>dublin</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Lists the available pages</description>
		<name>list-pages</name>
		<tag-class>com.finalist.cmsc.taglib.ListPagesTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put the page in.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Site or Page to start listing from</description>
			<name>origin</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Mode is all, menu, hidden. Default is menu</description>
			<name>mode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Lists the available ContentChannels</description>
		<name>list-contentchannels</name>
		<tag-class>com.finalist.cmsc.taglib.ListContentChannelsTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put the channels in.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Lists the available Portlet definitions</description>
		<name>list-portletdefinitions</name>
		<tag-class>com.finalist.cmsc.taglib.ListPortletDefinitionsTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put the portlet definitions in.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Lists the available ContentElements</description>
		<name>list-contentelements</name>
		<tag-class>com.finalist.cmsc.taglib.ListContentElementsTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put the views in.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>ContentChannel to start listing from</description>
			<name>origin</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Lists the available ContentTypes</description>
		<name>list-contenttypes</name>
		<tag-class>com.finalist.cmsc.taglib.ListContentTypesTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put the views in.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Insert related stylesheet</description>
		<name>insert-stylesheet</name>
		<tag-class>com.finalist.cmsc.taglib.InsertStylesheetTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>JSP variable to set stylesheet.</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Insert one of the images of a page</description>
		<name>insert-page-image</name>
		<tag-class>com.finalist.cmsc.taglib.InsertPageImageTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>JSP variable to set stylesheet.</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>The name of the position of the image (as given in namedrel builder)</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Generates urls for static content (No sessionid).</description>
		<name>staticurl</name>
		<tag-class>com.finalist.cmsc.taglib.StaticUrlTag</tag-class>
		<tei-class>org.mmbase.bridge.jsp.taglib.pageflow.UrlTEI</tei-class>
		<body-content>empty</body-content>
		<attribute>
			<name>referids</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>page</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>referid</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>escapeamps</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>encode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>absolute</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>jspvar</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>vartype</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>write</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>escape</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>context</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>cloud</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>	
		<description>Tag to format a date/dates</description>
		<name>dateformat</name>
		<tag-class>com.finalist.cmsc.taglib.DateFormatterTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
        	<name>begindate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>enddate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>displaytime</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<name>contenturl</name>
		<tag-class>com.finalist.cmsc.taglib.ContentUrlTag</tag-class>
		<tei-class>org.mmbase.bridge.jsp.taglib.StringWriterTEI</tei-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>element</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>node</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>number</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>jspvar</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>vartype</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>write</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>absolute</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>


<!-- ############## -->
<!-- Navigation Tags -->
<!-- ############## -->

	<tag>
		<description>Lists the available pages in the path</description>
		<name>path</name>
		<tag-class>com.finalist.cmsc.taglib.navigation.PathTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put the pages in.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Mode is all, menu, hidden. Default is menu</description>
			<name>mode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Make a link to a Page</description>
		<name>link</name>
		<tag-class>com.finalist.cmsc.taglib.navigation.LinkTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>
				Node (or list of), node-number or node-alias (or comma, slash or space separated
				list of) to navigation to.
			</description>
			<name>dest</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>content element number</description>
			<name>element</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>JSP variable name to put URL in.</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Tag to add a param to the surrounding link tag</description>
		<name>linkparam</name>
		<tag-class>com.finalist.cmsc.taglib.navigation.LinkParamTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Name of the parameter</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Value of this parameter.</description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>
			Tag to determine location by a given type, defaults to Page.
		   Gets a page object of a certain path or the current page
		</description>
		<name>location</name>
		<tag-class>com.finalist.cmsc.taglib.navigation.LocationTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>
				The CMSC name of the type node to find. This can be a comma separated list of
				nodemanager names.
			</description>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>JSP variable name to put the current NavChannel in.</description>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>JSP variable name to put the current Site in.</description>
			<name>sitevar</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>The optional path we want to check from, if empty defaults to the current path.</description>
			<name>path</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Check if a object is part ot the current path</description>
		<name>onpath</name>
		<tag-class>com.finalist.cmsc.taglib.navigation.OnPathTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Site or NavChannel.</description>
			<name>origin</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

<!-- ############## -->
<!-- Portlet Tags -->	
<!-- ############## -->

	<tag>
		<description>Container for a Portlet(Fragment) and it's modes</description>
		<name>portlet</name>
		<tag-class>com.finalist.cmsc.taglib.portlet.PortletTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>JSP variable name to put portlet information in.</description>
			<name>infovar</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Conditional tag which test for mode name</description>
		<name>portletmode</name>
		<tag-class>com.finalist.cmsc.taglib.portlet.PortletModeTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>name which should be matched</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Inverse the outcome of the test</description>
			<name>inverse</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>Conditional tag which test if the current user is allowed to edit this piece content</description>
		<name>isallowededit</name>
		<tag-class>com.finalist.cmsc.taglib.portlet.IsAllowedEditTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>the node number of the content channel</description>
			<name>channelNumber</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
      <attribute>
         <description>Inverse the result</description>
         <name>inverse</name>
         <required>false</required>
         <rtexprvalue>true</rtexprvalue>
      </attribute>
   </tag>
	<tag>
		<description>Helper for Portlet preferences</description>
		<name>portlet-preferences</name>
		<tag-class>com.finalist.cmsc.taglib.portlet.PortletPreferencesTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Name of preference</description>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>Value of preference if it has no value</description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description>
				JSP variable name to put a Map of all PortletPreferences in or the value if name is
				used.
			</description>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
    <tag>
        <name>param</name>
        <tag-class>com.finalist.cmsc.taglib.portlet.ParamTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
