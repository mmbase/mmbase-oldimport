    <tag>
        <name>renderURL</name>
        <tag-class>com.finalist.cmsc.taglib.portlet.RenderURLTag</tag-class>
        <tei-class>com.finalist.cmsc.taglib.portlet.BasicURLTag$TEI</tei-class>
        <body-content>JSP</body-content>
        <attribute>
            <name>page</name>
            <required>false</required>
