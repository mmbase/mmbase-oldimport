<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase/DTD builder 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder extends="insrel" maintainer="submarine.nl" name="posmboxrel" version="0">
	<status>active</status>
	<searchage>1000</searchage>
	<names>
		<!-- singles per language as defined by ISO 639 -->
		<singular xml:lang="en">PosMBoxRel</singular>
		<!-- singles per language as defined by ISO 639 -->
		<plural xml:lang="en">PosMBoxRels</plural>
	</names>
	<!-- <descriptions> small description of the builder for human reading -->
	<descriptions>
		<description xml:lang="en">Relation between posters and mailboxes, placeholder at this time</description>
	</descriptions>
	<fieldlist>
		<!-- POS 3 : <field> 'pos'  -->
		<field>
			<descriptions>
			</descriptions>
			<gui>
				<guitype>null</guitype>
			</gui>
			<!-- editor related  -->
			<editor>
				<positions>
					<!-- position in the input area of the editor -->
					<input>-1</input>
					<!-- position in list area of the editor -->
					<list>-1</list>
					<!-- position in search area of the editor -->
					<search>-1</search>
				</positions>
			</editor>
			<!-- database related  -->
			<db>
				<!-- name of the field in the database -->
				<name xml:lang="en">pos</name>
				<!-- MMBase datatype and demands on it -->
				<type key="false" notnull="false" state="persistent">INTEGER</type>
			</db>
		</field>
	</fieldlist>
</builder>
