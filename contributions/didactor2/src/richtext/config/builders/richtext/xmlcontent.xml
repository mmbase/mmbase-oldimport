<?xml version="1.0" ?>
<builder
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder
                        http://www.mmbase.org/xmlns/builder.xsd"
    name="xmlcontent" maintainer="mmbase.org" version="10"
    extends="learnobjects">
  <names>
    <singular xml:lang="nl">Inhoudsobject</singular>
    <singular xml:lang="en">Content object</singular>
    <singular xml:lang="eo">Enhavaĵo</singular>
    <plural xml:lang="nl">Inhoudsobjecten</plural>
    <plural xml:lang="en">Content objects</plural>
    <plural xml:lang="eo">Enhavaĵoj</plural>
  </names>
  <descriptions>
    <description xml:lang="nl">Inhoudsobjecten</description>
    <description xml:lang="en">Content objects</description>
    <description xml:lang="eo">Enhavaĵoj</description>
  </descriptions>

  <properties>
    <property name="key:new:name">richtext.createnewxmlcontent</property>
    <property name="key:new:description">richtext.createnewxmlcontentdescription</property>
    <property name="madetestholder">true</property>
  </properties>


  <xi:include href="rich.fields.xml" />


  <functionlist>
    <function key="questions">
      <class>nl.didactor.functions.RichQuestions</class>
    </function>
  </functionlist>

</builder>
