      </editor>
      <!-- database related  -->
      <db>
        <!-- name of the field in the database -->
        <name>start_time</name>
        <!-- MMBase datatype and demands on it -->
        <type state="persistent" notnull="false" key="false">INTEGER</type>
      </db>
    </field>
        
    <!-- POS 5 : <field> 'end time'  -->
    <field>
      <descriptions>
        <!-- descriptions per language as defined by ISO 639  -->
        <description xml:lang="en">End time</description>
        <description xml:lang="nl">De tijd van het eind</description>
