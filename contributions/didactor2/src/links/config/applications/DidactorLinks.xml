<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application PUBLIC "-//MMBase//DTD application config 1.1//EN" "http://www.mmbase.org/dtd/application_1_1.dtd">
<application name="DidactorLinks" maintainer="didactor.nl" version="1" auto-deploy="true">
    <requirements>
        <requires name="DidactorCore" maintainer="didactor.nl" version="8" />
        <requires name="DidactorComponent" maintainer="didactor.nl" version="2" />
        <requires name="DidactorResources" maintainer="didactor.nl" version="2" />
    </requirements>  

    <neededbuilderlist>
      <builder maintainer="didactor.nl" version="2">urls</builder>
      <builder maintainer="didactor.nl" version="2">pools</builder>
    </neededbuilderlist>

    <neededreldeflist>

    </neededreldeflist>

    <allowedrelationlist>
      <relation from="pools" to="urls" type="posrel" />
      <relation from="educations" to="pools" type="posrel" />
    </allowedrelationlist>
    <datasourcelist>
      <datasource builder="components" path="DidactorLinks/components.xml" />
    </datasourcelist>

    <relationsourcelist />

    <contextsourcelist />

    <description></description>

    <install-notice></install-notice>
</application>
