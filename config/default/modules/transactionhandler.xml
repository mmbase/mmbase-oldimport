<?xml version="1.0"?>
<!DOCTYPE module PUBLIC "//MMBase - module//" "http://www.mmbase.org/dtd/module.dtd">
<module>

  <!-- <status>
	what is the status of this builder options : active or inactive
  -->
  <status>active</status>


  <!-- <classfile>
	main classfile that this module is defined in
  -->
  <classfile>org.mmbase.module.TransactionHandler</classfile>

  <!-- <properties>
     you can define properties to be used by the classfile (if used) it uses
     a key/value system. Its a optional tag.
   -->
  <properties>
	<property name="keycode">hokusspokuspitaluspas</property>
	
	<!-- signal = if something goes wrong it will be printed
	     secure = the mallanous transaction will not be executed
	  -->
    <property name="security">signal</property>
  </properties>

</module>
