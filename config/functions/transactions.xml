<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE functionset PUBLIC "//MMBase - functionset //" "http://www.mmbase.org/dtd/functionset_1_0.dtd">
<functionset>
  <description>
    Functions related to MMBase transactions
  </description>
  <function name="list">
    <type>singleton</type>
    <class>org.mmbase.module.core.TransactionManager</class>
    <method>getTransactions</method>
  </function>
  <function name="get">
    <type>singleton</type>
    <class>org.mmbase.module.core.TransactionManager</class>
    <method>getTransaction</method>
    <param name="name" type="java.lang.String" />
  </function>
</functionset>
