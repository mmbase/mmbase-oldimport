			<name xml:lang="us">name</name>
			<!-- MMBase datatype and demands on it -->
			<type state="persistent" size="64" notnull="false" key="false">STRING</type>
		</db>
	</field>

	<!-- POS 4 : <field> 'id'  -->
	<field>
		<!-- gui related -->
		<gui>
			<guiname xml:lang="nl">ID</guiname>
			<guiname xml:lang="en">ID</guiname>
			<guiname xml:lang="us">ID</guiname>
